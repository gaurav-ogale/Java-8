package com.ho;

public class LamdaRunnableEx {

	public static void main(String[] args) {
		Thread t = new Thread(()-> {
			for(int i=0;i<10;i++) {
				System.out.println("i " + 1);
				try {
					Thread.sleep(4000);
				} catch (InterruptedException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			}
			
			
		});
	}
}
