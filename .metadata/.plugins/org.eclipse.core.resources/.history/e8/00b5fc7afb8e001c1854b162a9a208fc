package com.ct;

import java.util.HashMap;

public class EmployeeTest {
	public static void main(String[] args) {
		HashMap<Integer, Employee> emloyeeMap = new HashMap<>();
		
		emloyeeMap.put(1, new Employee(1,"Ron",112));
		emloyeeMap.put(2, new Employee(2,"Ron1",112));
		emloyeeMap.put(3, new Employee(3,"Ron2",112));
	}

}
